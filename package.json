{
  "name": "bookmark-delta-etl",
  "version": "1.0.0",
  "description": "Delta load ETL pipeline for bookmark data extraction and enrichment",
  "main": "run-bookmark-etl.ts",
  "scripts": {
    "dev:viewer": "bash scripts/dev_viewer.sh",
    "start": "bun run run-bookmark-etl.ts",
    "etl": "bun run run-bookmark-etl.ts",
    "etl:simple": "bun run run-bookmark-etl.ts --no-docetl",
    "etl:full": "bun run run-bookmark-etl.ts --content --social --temporal",
    "test": "bun run run-bookmark-etl.ts --batch 2 test_bookmarks.jsonl",
    "unified:run": "bun src/cli/unified.ts",
    "unified:verify": "bun src/cli/verify.ts",
    "unified:report": "bun src/cli/report.ts",
    "check": "tsc --noEmit",
    "env:check": "bun check-credentials.ts --test",
    "fetch:raindrop": "bun bin/fetch-raindrop.ts",
    "fetch:github": "bun bin/fetch-github-stars.ts",
    "fetch:all": "bun bin/fetch-all.ts",
    "audit:data": "bun scripts/data_audit.ts",
    "audit+preview": "bun scripts/data_audit.ts && echo 'Wrote reports/seed_preview.jsonl'",
    "ingest:convex": "bun apps/ingest/import_to_convex.ts --preview reports/seed_preview.jsonl --dry-run"
  },
  "dependencies": {
    "@ai-sdk/openai": "latest",
    "ai": "latest",
    "cheerio": "latest",
    "convex": "^1.25.4",
    "zod": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "typescript": "latest"
  }
}
